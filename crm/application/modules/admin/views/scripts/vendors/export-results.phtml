<script type="text/javascript">
    $(window).on('load',function(){
        $('input[name^=export_fields]').attr('checked','checked');
    });
    $(document).on('click','#check',function(){
        $('input[name^=export_fields]').attr('checked','checked');
    });
    $(document).on('click','#uncheck',function(){
        $('input[name^=export_fields]').removeAttr('checked');
    });
    $(document).on('click','#export',function(){
        all_values="";
        $('input[name^=export_fields]:checked').each(function(){
            all_values+=$(this).val().split(".")[1]+"~"+$.trim($(this).parent().text())+"||";
        });
        $("#labels").val(all_values);
        window.parent.export_result($('#export_form').serialize());
    });
</script>
